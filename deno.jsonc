{
  "fmt": {
    "exclude": ["CHANGELOG.md"]
  },
  "lint": {
    "rules": {
      "include": ["explicit-module-boundary-types"]
    }
  },
  "tasks": {
    "fmt": "deno fmt",
    "lint": "deno lint",
    "cache": "deno cache --lock=deno.lock --lock-write core/deps.ts utils/deps.ts",
    "reload": "deno cache --reload --lock=deno.lock core/deps.ts utils/deps.ts"
  }
}
